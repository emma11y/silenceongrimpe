{
  "version": 2,
  "framework": "angular",
  "outputDirectory": "dist/silenceongrimpe",
  "buildCommand": "npm run vercel-build",
  "nodeVersion": "18.x",
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/browser/assets/:path*"
    },
    {
      "source": "/:path*.js",
      "destination": "/browser/:path*.js"
    },
    {
      "source": "/:path*.css",
      "destination": "/browser/:path*.css"
    },
    {
      "source": "/:path*.ico",
      "destination": "/browser/:path*.ico"
    },
    {
      "source": "/:path*.png",
      "destination": "/browser/:path*.png"
    },
    {
      "source": "/:path*.webp",
      "destination": "/browser/:path*.webp"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/**/*": {
      "runtime": "@vercel/node@3.0.0"
    }
  }
}
