{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.mjs"
    }
  ]
}
