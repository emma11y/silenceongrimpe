{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/silenceongrimpe",
  "functions": {
    "api/index.mjs": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|webp))",
      "dest": "/$1"
    },
    {
      "src": "/((?!api/).*)",
      "dest": "/api/index.mjs"
    }
  ]
}
