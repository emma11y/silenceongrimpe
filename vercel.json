{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)\\.(?:js|css|ico|png|webp)$",
      "destination": "/$1.$2"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/**/*": {
      "runtime": "@vercel/node@3.0.0"
    }
  }
}
