{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "^/(?:assets|favicon\\.ico|site\\.webmanifest|.*\\.(?:js|css|json|png|jpg|jpeg|gif|svg|ico))$",
      "dest": "/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
