{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/:file(.*\\.js|.*\\.css|.*\\.ico|.*\\.png|.*\\.webp)",
      "destination": "/:file"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/**/*": {
      "runtime": "@vercel/node@3.0.0"
    }
  }
}
