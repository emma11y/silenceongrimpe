{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "^/assets/(.*)", "dest": "/browser/assets/$1" },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/browser/index.html" }
  ]
}
