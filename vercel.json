{
  "version": 2,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/dist/silenceongrimpe/browser/assets/$1"
    },
    {
      "source": "/(.*)\\.([a-z]{2,4})$",
      "destination": "/dist/silenceongrimpe/browser/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ]
}
