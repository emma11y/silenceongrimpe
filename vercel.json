{
  "version": 2,
  "routes": [
    {
      "src": "^/assets/(.*)$",
      "dest": "/dist/silenceongrimpe/browser/assets/$1"
    },
    {
      "src": "^/(.*)\\.(css|js|ico|png|jpg|jpeg|svg|webp)$",
      "dest": "/dist/silenceongrimpe/browser/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/silenceongrimpe/browser/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ]
}
